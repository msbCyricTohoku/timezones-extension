{
    "manifest_version": 3,
    "name": "Simple Time Zone Clock",
    "version": "1.0",
    "description": "Displays the current time in Local, UTC, JST, and NYC time zones.",
    "icons": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png"
      }
    }
  }